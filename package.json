{
  "name": "@indimeco/eslint-config-indimeco",
  "version": "1.1.9",
  "description": "eslint config for my projects",
  "author": "Indimeco",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier",
    "prettier-config"
  ],
  "bugs": {
    "url": "https://github.com/indimeco/eslint-config-indimeco/issues"
  },
  "homepage": "https://github.com/indimeco/eslint-config-indimeco#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/indimeco/eslint-config-indimeco"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "prettier.js",
    "prettier.config.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{*.js, *.json}": "npx eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": ">=6.5.1 <7",
    "eslint-config-airbnb": ">=18.0.1 <19",
    "eslint-config-prettier": ">=6.4.0 <7",
    "eslint-plugin-import": ">=2.18.2 <3",
    "eslint-plugin-jest": ">=22.19.0 <23",
    "eslint-plugin-jsx-a11y": ">=6.2.3 <7",
    "eslint-plugin-prettier": ">=3.1.1 <4",
    "eslint-plugin-react": ">=7.16.0 <8",
    "eslint-plugin-react-hooks": ">=2.1.2 <3",
    "prettier": ">=1.18.2 <2",
    "typescript": "3.7.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": ">=6.5.1 <7",
    "eslint-config-airbnb": ">=18.0.1 <19",
    "eslint-config-prettier": ">=6.4.0 <7",
    "eslint-plugin-import": ">=2.18.2 <3",
    "eslint-plugin-jest": ">=22.19.0 <23",
    "eslint-plugin-jsx-a11y": ">=6.2.3 <7",
    "eslint-plugin-prettier": ">=3.1.1 <4",
    "eslint-plugin-react": ">=7.16.0 <8",
    "eslint-plugin-react-hooks": ">=2.1.2 <3",
    "prettier": ">=1.18.2 <2",
    "typescript": "3.7.4"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "commitlint": "^8.3.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "semantic-release": "^17.0.4"
  }
}
