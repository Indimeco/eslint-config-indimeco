{
  "name": "@indimeco/eslint-config-indimeco",
  "version": "1.1.9",
  "description": "eslint config for my projects",
  "author": "Indimeco",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslint-config",
    "prettier",
    "prettier-config"
  ],
  "bugs": {
    "url": "https://github.com/indimeco/eslint-config-indimeco/issues"
  },
  "homepage": "https://github.com/indimeco/eslint-config-indimeco#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/indimeco/eslint-config-indimeco"
  },
  "main": "index.js",
  "files": [
    "index.js",
    "prettier.js",
    "prettier.config.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "{*.js, *.json}": "npx eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "eslint": "^7",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4",
    "prettier": "^2",
    "typescript": "^4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "eslint": "^7",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4",
    "prettier": "^2",
    "typescript": "^4",
    "@commitlint/config-conventional": "^8.3.4",
    "commitlint": "^8.3.5",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "semantic-release": "^17.0.4"
  },
  "dependencies": {}
}
